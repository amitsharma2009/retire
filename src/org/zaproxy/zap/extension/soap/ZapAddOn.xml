<zapaddon>
	<name>Imports and scans WSDL files containing SOAP endpoints.</name>
	<version>1</version>
	<description>Adds an option to import WSDL file with SOAP operations. The file must be a well formed WSDL file.</description>
	<author>Alberto (albertov91)</author>
	<url></url>
	<changes>First version</changes>
	<dependson/>
	<extensions>
	    <extension>org.zaproxy.zap.extension.soap.ExtensionImportWSDL</extension>
	</extensions>
	<ascanrules>
	  	<ascanrule>org.zaproxy.zap.extension.soap.SOAPActionSpoofingActiveScanner</ascanrule>
	</ascanrules>
	<pscanrules>
	    <pscanrule>org.zaproxy.zap.extension.soap.WSDLFilePassiveScanner</pscanrule>
	</pscanrules>
	<filters/>
	<files/>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>
