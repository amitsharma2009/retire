<zapaddon>
	<name>SNI Terminator</name>
	<version>2</version>
	<description>Transparent HTTP proxying - requires Java 8!</description>
	<author>ZAP Dev Team</author>
	<url>http://www.computerist.org/blog/2014/07/23/Transparent-HTTPS-proxying-with-ZAP/</url>
	<changes>Call start explicitly as its no longer called from the constructor</changes>
	<dependson/>
	<extensions>
		<extension>org.zaproxy.zap.extension.sniTerminator.ExtensionSniTerminator</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.3.0</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>

